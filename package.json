{
  "name": "flavor-js",
  "version": "0.4.0",
  "description": "FlavorJS the definitive JS natives chainable extensions methods (based on lodash & ES6)",
  "main": "dist/flavor.min.js",
  "scripts": {
    "docs": "rm -rf docs && jsdoc -R ./README.md -r -c ./jsdoc.json && echo 'Documentation built in ./docs folder'",
    "build": "clear && webpack --progress",
    "build:docs": "npm run build && npm run docs",
    "build:readme": "pkg-2-readme -b -t \"./pkg-2-readme.template.md\" package.json > README.md",
    "build:release": "npm version patch && npm run build:readme && npm run build:docs",
    "postinstall": "clear && cd node_modules/ink-docstrap && npm i && cd ../docdash && npm i && cd ../.. && clear"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blackmirror1980/flavor-js.git"
  },
  "keywords": [
    "extend",
    "natives",
    "javascript",
    "flavor-js",
    "ES6"
  ],
  "author": "Black Mirror",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blackmirror1980/flavor-js/issues"
  },
  "homepage": "https://github.com/blackmirror1980/flavor-js#readme",
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.17",
    "compression-webpack-plugin": "^1.1.3",
    "docdash": "^0.4.0",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.5.5",
    "lodash-webpack-plugin": "^0.11.4",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "webpack-ver-plugin": "0.0.2"
  }
}
